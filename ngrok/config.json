{
  "name": "ngrok Client",
  "version": "v1.0.3",
  "slug": "ngrok",
  "description": "A ngrok client for Hass.io",
  "webui": "http://[HOST]:[PORT:4040]",
  "url": "https://github.com/ThePicklenat0r/hassio-ngrok",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "ports": {
    "4040/tcp": "4040"
  },
  "ports_description": {
    "4040/tcp": "Web interface"
  },
  "map": ["share"],
  "options": {
    "auth_token": "",
    "region": "us",
    "port": 8123,
    "use_tls": false,
    "inspect": false
  },
  "schema": {
    "auth_token": "str?",
    "region": "list(us|eu|ap|au|sa|jp|in)?",
    "port": "int(1,65535)?",
    "use_tls": "bool?",
    "inspect": "bool?",
    "subdomain": "str?",
    "hostname": "str?"
  },
  "image": "thepicklenat0r/{arch}-hassio-ngrok"
}
